    "file-saver": "^2.0.5",
    "howler": "^2.2.4",
    "jquery": "^3.7.1",
    "jszip": "^3.10.1",
    "maxmind-db-reader": "^0.2.1",
    "moment-jalaali": "^0.10.0",
    "mongodb": "^6.8.0",
    "next": "^15.1.5",
    "node-schedule": "^2.1.1",
    "nodemailer": "^6.9.15",
    "nprogress": "^0.2.0",
    "pako": "^2.1.0",
