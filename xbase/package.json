{
  "name": "ebase-xbase",
  "version": "0.1.0",
  "private": true,
  "description": "QE Lite",
  "homepage": "https://qepal.com",
  "author": "Ethan Cardan <armin.fire@gmail.com>",
  "license": "ISC",
  "scripts": {
    "dev": "cmd /C \"npx ts-node --transpileOnly ./sync.ts && set CIRCLE_NODE_TOTAL=1 && set DEVMODE=true && set NEXT_MANUAL_SIG_HANDLE=1 && next dev\"",
    "build": "cmd /C \"npx ts-node --transpileOnly ./sync.ts && set CIRCLE_NODE_TOTAL=1 && set ANALYZE=true && set DEVMODE=true && set BUILDMODE=true && set NEXT_MANUAL_SIG_HANDLE=1 && next build\"",
    "start": "cmd /C \"npx ts-node --transpileOnly ./sync.ts && set CIRCLE_NODE_TOTAL=1 && set DEVMODE=false && set STARTMODE=true && set NEXT_MANUAL_SIG_HANDLE=1 && next start\"",
    "devlinux": "CIRCLE_NODE_TOTAL=1 DEVMODE=true NEXT_MANUAL_SIG_HANDLE=1 next dev",
    "buildlinux": "CIRCLE_NODE_TOTAL=1 ANALYZE=true DEVMODE=true BUILDMODE=true NEXT_MANUAL_SIG_HANDLE=1 next build",
    "startlinux": "CIRCLE_NODE_TOTAL=1 DEVMODE=false STARTMODE=true NEXT_MANUAL_SIG_HANDLE=1 next start",
    "lint": "next lint",
    "types": "tsc --noEmit"
  },
  "dependencies": {
    "2captcha": "^3.0.5-2",
    "@noble/secp256k1": "^2.1.0",
